{
  "expo": {
    "name": "test-frontend",
    "slug": "test-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SafeDrive to access your camera to perform the eye tracking test."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow SafeDrive to access your media library to save test recordings.",
          "savePhotosPermission": "Allow SafeDrive to save test recordings to your media library."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
